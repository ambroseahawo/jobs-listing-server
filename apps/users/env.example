# Users Service Environment Variables
NODE_ENV=development
PORT=3002

# Database Configuration
DATABASE_URL="postgresql://users_user:users_password@users-postgres:5432/users_db?schema=public"

# RabbitMQ Configuration
RABBIT_MQ_URI=amqp://rabbitmq:5672
RABBIT_MQ_USERS_QUEUE=users_queue

# JWT Configuration (for service-to-service auth)
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=7d

# Redis Configuration
REDIS_URL=redis://redis:6379 